clear; close all; clc

% Define variables
syms p f g h k L real % Modified Eq. Elements
syms a e inc ran aop nu real % Kep. Elemenents

% Make assumptions
assume()

eqs = [p == a*(1 - e^2);
       f == e*cos(aop + ran);
       g == e*sin(aop + ran);
       h == tan(inc/2)*cos(ran);
       k == tan(inc/2)*sin(ran);
       L == ran + aop + nu];


seqs = solve(eqs, [a,e,inc,ran,aop,nu]);


